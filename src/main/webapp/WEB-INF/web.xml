<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Archetype Created Web Application</display-name>
    <servlet>
        <servlet-name>GenericAdapter</servlet-name>
        <servlet-class>cloudlab.genericadapter.Adapter</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ReadmeGenerator</servlet-name>
        <servlet-class>cloudlab.genericadapter.ReadmeGenerator</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>GenericAdapter</servlet-name>
        <url-pattern>/request</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ReadmeGenerator</servlet-name>
        <url-pattern>/readme</url-pattern>
    </servlet-mapping>
</web-app>

